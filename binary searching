# include<iostream>
using namespace std;
int main()
{
    int matrix [3][3] ;
    int p;
    int low =0;
    int high =8;
    bool found=false;
     
    cout<<"Enter matrix:"<<endl;
    for(int i=0;i<3;i++)
    {
        for(int j=0;j<3;j++)
        {
            cin>>matrix[i][j];
        }
    }
    
    cout<<"Enter value of p:"<<endl;
    cin>>p;
    while(low<=high)
   {
    int mid =(low+high)/2;
    int value=matrix[mid/3][mid%3];

    if(value==p)
    {
        cout<<"found";
        found=true;
        break;
    }
    else if(value>p)
    {
        high=mid-1;
    }
    else if(value<p)
    {
        low=mid+1;
    }
  

   }
   if(!found)
   {
   cout<<"not found";
}
   return 0;
    
}
