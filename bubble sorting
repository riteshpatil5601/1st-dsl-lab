#include <iostream>
using namespace std;

int main()
{
    int n;
    int arr[10];

    cout << "ENTER VALUE FOR N: ";
    cin >> n;

    cout << "ENTER N NUMBERS:" << endl;
    for (int i = 0; i < n; i++)
    {
        cin >> arr[i];
    }

    
    for (int i = 0; i < n; i++)
    {
        for (int j = i + 1; j < n; j++)
        {
            if (arr[j] < arr[i])
            {
                int t = arr[i];
                arr[i] = arr[j];
                arr[j] = t;
            }
        }
    }

    cout << "SORTED LIST:" << endl;
    for (int i = 0; i < n; i++)
    {
        cout << arr[i] << " ";
    }

    return 0;
}
