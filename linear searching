#include <iostream>
using namespace std;

int main()
{
    int matrix[2][2] ;
   bool found = false;
    int s;
    cout<<"Enter matrix:"<<endl;
    for(int i=0;i<2;i++)
    {
        for(int j=0;j<2;j++)
        {
            cin>>matrix[i][j];
        }
    }
    cout<<"Enter s:"<<endl;
    cin>>s;

    for(int i=0;i<2;i++)
    {
        for(int j=0;j<2;j++)
        {
            if (matrix[i][j]==s)
            {
                found = true;
                break;
             }
            
        }

    }
            if (found==true)
               {
                cout<<"Found";
             }

            else
                {
                cout<<"Not Found";
             }

}
